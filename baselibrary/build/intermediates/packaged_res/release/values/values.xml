        <item name="android:windowExitAnimation">@anim/translate_y_0_to_n1_time_600</item>
    </style>
    <declare-styleable name="Banner">
        <attr format="reference" name="default_image"/>
        <attr format="dimension" name="delay_time"/>
        <attr format="boolean" name="is_auto_play"/>
        <attr format="dimension" name="indicator_width"/>
        <attr format="dimension" name="indicator_height"/>
        <attr format="dimension" name="indicator_margin"/>
        <attr format="reference" name="indicator_drawable_selected"/>
        <attr format="reference" name="indicator_drawable_unselected"/>
        <!--<attr name="indicator_animator" format="reference" />-->
        <!--<attr name="indicator_animator_reverse" format="reference" />-->
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr format="dimension" name="civ_border_width"/>
        <attr format="color" name="civ_border_color"/>
        <attr format="boolean" name="civ_border_overlay"/>
        <attr format="color" name="civ_fill_color"/>
    </declare-styleable>
    <declare-styleable name="SlideDetailsLayout">
        <!-- float value for indicate the moment of switch panel-->
        <attr format="float" name="percent"/>
        <!-- how long the animation keep-->
        <attr format="integer" name="duration"/>
        <!-- default panel to show after init-->
        <attr format="enum" name="default_panel">
            <enum name="front" value="0"/>
            <enum name="behind" value="1"/>
        </attr>
    </declare-styleable>
