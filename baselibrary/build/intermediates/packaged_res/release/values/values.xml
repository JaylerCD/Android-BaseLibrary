        <item name="android:windowExitAnimation">@anim/translate_x_0_to_n1_time_400</item>
    </style>
    <style name="DialogAnimationLeftInTime600" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/translate_x_n1_to_0_time_600</item>
        <item name="android:windowExitAnimation">@anim/translate_x_0_to_n1_time_600</item>
    </style>
    <style name="DialogAnimationRightInTime200" parent="android:Animation">
